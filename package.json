{
  "name": "wix-to-wordpress-importer",
  "version": "0.0.1",
  "description": "Import Wix blog posts to WordPress",
  "bin": "lib/index.js",
  "type": "module",
  "scripts": {
    "pretest": "rimraf wordpress-import-*.xml",
    "test": "node lib/index.js --config .config.json",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "ci:test": "npm run format && npm run lint",
    "preversion": "npm run ci:test",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "wordpress",
    "wix",
    "wxr",
    "import",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bigtimebuddy/wix-to-wordpress-importer.git"
  },
  "bugs": {
    "url": "https://github.com/bigtimebuddy/wix-to-wordpress-importer/issues"
  },
  "homepage": "https://github.com/bigtimebuddy/wix-to-wordpress-importer#readme",
  "author": "Matt Karl <hello@mattkarl.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.3",
    "chalk": "^5.4.1",
    "cheerio": "^1.0.0",
    "fs-extra": "^11.3.0",
    "minimist": "^1.2.8",
    "progress": "^2.0.3",
    "sanitize-html": "^2.14.0",
    "xml2js": "^0.6.2",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/sanitize-html": "^2.13.0",
    "clean-package": "^2.2.0",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "prettier",
      "scripts"
    ]
  }
}
